---
import ContentLayout from '../../../layouts/ContentLayout.astro'
import { MANUAL_SIDEBAR } from '../../../scripts/sidebars'
import { getEntry } from 'astro:content'

const entry = await getEntry('docs', '99-manual/cherrystudio')
const rendered = entry && (await entry.render())
---

<ContentLayout
  entry={entry}
  title={entry?.data?.title ?? 'CherryStudio'}
  sidebarItems={MANUAL_SIDEBAR}
  contributors={entry?.data?.contributors}
>
  {rendered ? <rendered.Content /> : <div>文档未找到或渲染失败</div>}
</ContentLayout>
