---
import ContentLayout from "../../layouts/ContentLayout.astro"
import { PROMPTS_SIDEBAR } from "../../scripts/sidebars"
import { getEntry } from "astro:content"

const entry = await getEntry("docs", "03-prompts/context-learning")
const rendered = entry && (await entry.render())
---

<ContentLayout title={entry?.data?.title ?? "上下文学习"} section="提示词" sidebarItems={PROMPTS_SIDEBAR}>
  {rendered ? <rendered.Content /> : <div>文档未找到或渲染失败</div>}
</ContentLayout>

