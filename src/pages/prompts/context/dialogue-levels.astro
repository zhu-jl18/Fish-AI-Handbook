---
import ContentLayout from '../../../layouts/ContentLayout.astro'
import { getEntry } from 'astro:content'

const entry = await getEntry('docs', '03-prompts/context/dialogue-levels')
if (!entry) {
  throw new Error('Content not found: 03-prompts/context/dialogue-levels')
}
const { Content } = await entry.render()
---

<ContentLayout
  entry={entry}
  title={entry?.data?.title || '对话层级'}
  headings={[]}
  contributors={entry?.data?.contributors}
>
  <Content />
</ContentLayout>
