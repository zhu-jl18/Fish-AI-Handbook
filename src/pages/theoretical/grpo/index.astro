---
import ContentLayout from '../../../layouts/ContentLayout.astro'
import { THEORETICAL_SIDEBAR } from '../../../scripts/sidebars'
import { getEntry } from 'astro:content'

const entry = await getEntry('docs', '07-theoretical/grpo')
const rendered = entry && (await entry.render())
---

<ContentLayout
  title={entry?.data?.title ?? 'GRPO'}
  section="理论学习"
  sidebarItems={THEORETICAL_SIDEBAR}
  contributors={entry?.data?.contributors}
>
  {rendered ? <rendered.Content /> : <div>文档未找到或渲染失败</div>}
</ContentLayout>

