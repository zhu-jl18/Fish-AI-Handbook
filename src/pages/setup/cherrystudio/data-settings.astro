---
import ContentLayout from "../../../layouts/ContentLayout.astro"
import { SETUP_SIDEBAR } from "../../../scripts/sidebars"
import { getEntry } from "astro:content"

const entry = await getEntry("docs", "03-setup/cherrystudio/data-settings")
const rendered = entry && (await entry.render())
---

<ContentLayout
  title={entry?.data?.title ?? "CherryStudio · 数据设置"}
  section="配置指南"
  sidebarItems={SETUP_SIDEBAR}
  headings={[]}
>
  {rendered ? <rendered.Content /> : <div>文档未找到或渲染失败</div>}
</ContentLayout>
