---
import ContentLayout from '../../../layouts/ContentLayout.astro'
import { SETUP_SIDEBAR } from '../../../scripts/sidebars'
import { getEntry } from 'astro:content'

const entry = await getEntry('docs', '99-setup/cherrystudio')
const { Content } = await entry.render()
---

<ContentLayout title={entry?.data?.title ?? "CherryStudio"} section="配置指南" sidebarItems={SETUP_SIDEBAR}>
  <Content />
</ContentLayout>
