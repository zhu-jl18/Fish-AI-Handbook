---
import ContentLayout from '../../layouts/ContentLayout.astro'
import { SETUP_SIDEBAR } from '../../scripts/sidebars'
import { getEntry } from 'astro:content'

const entry = await getEntry('docs', '10-setup/vpn')
let Content: any
if (entry) {
  const rendered = await entry.render()
  Content = rendered.Content
} else {
  Content = () => null
}

---

<ContentLayout
  title={entry?.data?.title ?? 'VPN'}
  section="配置指南"
  sidebarItems={SETUP_SIDEBAR}
>
  <Content />
</ContentLayout>
