---
import ContentLayout from '../../../layouts/ContentLayout.astro'
import { SETUP_SIDEBAR } from '../../../scripts/sidebars'
import { getEntry } from 'astro:content'

const entry = await getEntry('docs', '99-setup/vpn/cloud-server')
const rendered = entry && (await entry.render())
---

<ContentLayout
  entry={entry} title={entry?.data?.title ?? '云服务器'}
  sidebarItems={SETUP_SIDEBAR}
  contributors={entry?.data?.contributors}
>
  {rendered ? <rendered.Content /> : <div>文档未找到或渲染失败</div>}
</ContentLayout>
