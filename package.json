{
  "name": "fish-ai-book",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && pagefind --site dist --exclude-selectors \"header,nav,footer,.left-sidebar,.right-sidebar,[data-pagefind-ignore='true']\"",
    "preview": "astro preview",
    "preview:search": "npm run build && astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:links": "npm run build && linkinator ./dist --config linkinator.config.json --fail"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.6",
    "@pagefind/default-ui": "^1.4.0",
    "astro": "^5.13.10",
    "fish-ai-book": "file:"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "linkinator": "^7.1.3",
    "pagefind": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1"
  }
}
