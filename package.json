{
  "name": "fish-ai-book",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && pagefind --site dist --exclude-selectors \"header,nav,footer,.left-sidebar,.right-sidebar,[data-pagefind-ignore='true']\"",
    "preview": "astro preview",
    "preview:search": "npm run build && astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "lint:markdown": "node ./scripts/lint-markdown-code-blocks.mjs",
    "type-check": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:links": "npm run build && linkinator ./dist/index.html --recurse --config linkinator.config.json --fail --skip \"^https?://\" --skip \"^mailto:\" --skip \"^tel:\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.6",
    "@pagefind/default-ui": "^1.4.0",
    "astro": "^5.13.10",
    "astro-expressive-code": "^0.41.3",
    "fish-ai-book": "file:",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "linkinator": "^7.1.3",
    "pagefind": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1"
  }
}
