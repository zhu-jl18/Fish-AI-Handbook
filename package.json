{
  "name": "fish-ai-book",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && pagefind --site dist --exclude-selectors \"header,nav,footer,.left-sidebar,.right-sidebar,[data-pagefind-ignore='true']\"",
    "preview": "astro preview",
    "preview:search": "npm run build && astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "lint:markdown": "node ./scripts/lint-markdown-code-blocks.mjs",
    "type-check": "astro check",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:links": "npm run build && node scripts/run-link-check.js",
    "check:routes": "node scripts/check-route-structure.js",
    "check:page-structure": "node scripts/check-runner.mjs page-structure",
    "check:all": "node scripts/check-runner.mjs all"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.6",
    "@astrojs/rss": "^4.0.7",
    "@pagefind/default-ui": "^1.4.0",
    "astro": "^5.13.10",
    "astro-expressive-code": "^0.41.3",
    "fish-ai-book": "file:",
    "katex": "^0.16.25",
    "rehype-katex": "^7.0.1",
    "remark-directive": "^4.0.0",
    "remark-math": "^6.0.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^3.6.0",
    "@playwright/test": "^1.48.2",
    "linkinator": "^7.1.3",
    "pagefind": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1"
  }
}
